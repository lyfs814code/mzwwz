<!DOCTYPE html>
<html>
<head>
<script type="text/javascript">
	$(function() {
		$("#jvForm").bootstrapValidator({
			feedbackIcons: {
	            valid: 'glyphicon glyphicon-ok',
	            invalid: 'glyphicon glyphicon-remove',
	            validating: 'glyphicon glyphicon-refresh'
	        },
			fields:  {
								/*id:{
	            	validators: {
	                	
	            	}
	            }, */
								newsType:{
	            	validators: {
	        			}        	
	            	}
	            },
								title:{title:{
	            	validators: {
	                	notEmpty: {
	                        message: '标题不能为空'
	                    }
	            	}
	            },
								source:{
	            	validators: {
	                	notEmpty: {
	                        message: '来源不能为空'
	                    }
	            	}
	            },
								detail:{
	            	validators: {
	                	notEmpty: {
	                        message: '内容不能为空'
	                    }
	            	}
	            },
								publishFlag:{
	            	validators: {
	                	
	            	}
	            },
								creater:{
	            	validators: {
	                	
	            	}
	            },
								/* createTime:{
	            	validators: {
	                	
	            	}
	            }, */
								/* modifier:{
	            	validators: {
	                	
	            	}
	            },
								modifyTime:{
	            	validators: {
	                	
	            	}
	            },
							 */},
			submitHandler: function(validator, form, submitButton) {
				$.web.submit("jvForm");
			}
		});
	});
</script>
　<script type="text/javascript">
		$('#datetimepicker').datetimepicker({
		format: 'yyyy-mm-dd '      /*此属性是显示顺序，还有显示顺序是mm-dd-yyyy*/
		});
	</script>
    </body>	
</head>
<body>
	<div class="row">
		<div class="col-lg-12">
			<!-- 页面信息 -->
			<div class="console-title console-title-border clearfix">
				<div class="pull-left">
					<h3>新增近期焦点</h3>
				</div>
				<div class="pull-right">[@p.history_back/]</div>
			</div>
			<!-- 新增表单 -->
			[@p.form id="jvForm" action="newJqjd/save.do"]
				<div class="clearfix">
				<!-- 	<div class="form-group col-sm-12 col-lg-3" style="display:none ">
						<label>id</label>
						<input type="text" class="form-control" name="id"  value=""/>	
					</div>  -->
					<div class="form-group col-sm-12 col-lg-3" style="display:none ">
						 <label>县区动态</label>
						 <!-- 图片新闻 ，状态为3 -->
						<input type="hidden" class="form-control" name="newsType"  value="3"/>	
					</div>
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>标题</label>
						<input type="text" class="form-control" name="title"  value=""/>	
					</div>
				</div>
				<div class="form-group col-sm-12 col-lg-3">
						<label>来源</label>
						<input type="text" class="form-control" name="source"  value=""/>	
					</div>
						<br><br><br><br>
					<div class="form-group col-sm-12 col-lg-3">
						<label>是否发布</label>
						[@p.constant_radio code_type="IS_PUBLISH" id="publishFlag" name="publishFlag" value="1"/]
					</div>
				<br><br>
				<div class="clearfix">
					<div class="form-group col-lg-12">
						<label>内容</label>
						[@p.editor_baidu name="detail" id="detail" value=""/]
					</div>
				</div>
				</div>
				<div class="clearfix">
				</div>
				<div class="clearfix">
					<div class="col-sm-12 col-lg-8">[@p.submit/][@p.cancel/]</div>
				</div>
				[/@p.form]
		</div>
	</div>
</body>
</html>
