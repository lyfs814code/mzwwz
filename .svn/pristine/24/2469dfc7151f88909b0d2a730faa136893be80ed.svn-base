<!DOCTYPE html>
<html>
<head>
<script type="text/javascript">
	$(function() {
		$("#jvForm").bootstrapValidator({
			feedbackIcons: {
	            valid: 'glyphicon glyphicon-ok',
	            invalid: 'glyphicon glyphicon-remove',
	            validating: 'glyphicon glyphicon-refresh'
	        },
			fields: {
								id:{
	            	validators: {
	                	
	            	}
	            },
								paraCode:{
	            	validators: {
	                	
	            	}
	            },
								paraName:{
	            	validators: {
	                	
	            	}
	            },
								paraValue:{
	            	validators: {
	                	
	            	}
	            },
								modifier:{
	            	validators: {
	                	
	            	}
	            },
								modifyTime:{
	            	validators: {
	                	
	            	}
	            },
							},
			submitHandler: function(validator, form, submitButton) {
				$.web.submit("jvForm");
			}
		});
	});
</script>
</head>
<body>
	<div class="row">
		<div class="col-lg-12">
			<!-- 页面信息 -->
			<div class="console-title console-title-border clearfix">
				<div class="pull-left">
					<h3>sysPara信息修改</h3>
				</div>
				<div class="pull-right">[@p.history_back/]</div>
			</div>
			<!-- 编辑表单 -->
			[@p.form id="jvForm" action="sysPara/update.do"]
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>id</label>				
						<input type="text" class="form-control" name="id"  value="${sysPara.id!}"/>
					</div>							
					<div class="form-group col-sm-12 col-lg-3">
						<label>paraCode</label>				
						<input type="text" class="form-control" name="paraCode"  value="${sysPara.paraCode!}"/>
					</div>							
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>paraName</label>				
						<input type="text" class="form-control" name="paraName"  value="${sysPara.paraName!}"/>
					</div>							
					<div class="form-group col-sm-12 col-lg-3">
						<label>paraValue</label>				
						<input type="text" class="form-control" name="paraValue"  value="${sysPara.paraValue!}"/>
					</div>							
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>modifier</label>				
						<input type="text" class="form-control" name="modifier"  value="${sysPara.modifier!}"/>
					</div>							
					<div class="form-group col-sm-12 col-lg-3">
						<label>modifyTime</label>				
						<input type="text" class="form-control" name="modifyTime"  value="${(sysPara.modifyTime?string('yyyy-MM-dd'))!''}"/>
					</div>							
				</div>
				<div class="clearfix">
				<div class="col-sm-12 col-lg-8">[@p.submit /][@p.cancel/]</div>
			</div>
			[/@p.form]
		</div>
	</div>
</body>
</html>
</html>
