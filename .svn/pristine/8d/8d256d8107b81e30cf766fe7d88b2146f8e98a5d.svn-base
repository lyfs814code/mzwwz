<!DOCTYPE html>
<html>
<head>
<script type="text/javascript">
	$(function() {
		$("#jvForm").bootstrapValidator({
			feedbackIcons: {
	            valid: 'glyphicon glyphicon-ok',
	            invalid: 'glyphicon glyphicon-remove',
	            validating: 'glyphicon glyphicon-refresh'
	        },
			fields: {
								codeType:{
	            	validators: {
	                	
	            	}
	            },
								codeName:{
	            	validators: {
	                	
	            	}
	            },
							},
			submitHandler: function(validator, form, submitButton) {
				$.web.submit("jvForm");
			}
		});
	});
</script>
</head>
<body>
	<div class="row">
		<div class="col-lg-12">
			<!-- 页面信息 -->
			<div class="console-title console-title-border clearfix">
				<div class="pull-left">
					<h3>sysCodeType信息修改</h3>
				</div>
				<div class="pull-right">[@p.history_back/]</div>
			</div>
			<!-- 编辑表单 -->
			[@p.form id="jvForm" action="sysCodeType/update.do"]
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>codeType</label>				
						<input type="text" class="form-control" name="codeType"  value="${sysCodeType.codeType!}"/>
					</div>							
					<div class="form-group col-sm-12 col-lg-3">
						<label>codeName</label>				
						<input type="text" class="form-control" name="codeName"  value="${sysCodeType.codeName!}"/>
					</div>							
				</div>
				<div class="clearfix">
				<div class="col-sm-12 col-lg-8">[@p.submit /][@p.cancel/]</div>
			</div>
			[/@p.form]
		</div>
	</div>
</body>
</html>
</html>
