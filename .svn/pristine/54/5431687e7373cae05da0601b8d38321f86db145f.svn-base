<script type="text/javascript">
		$('#datetimepicker').datetimepicker({
		format: 'yyyy-mm-dd '      /*此属性是显示顺序，还有显示顺序是mm-dd-yyyy*/
		});
</script>
</head>
<body>
	<div class="row">
		<div class="col-lg-12">
			<!-- 页面信息 -->
			<div class="console-title console-title-border clearfix">
				<div class="pull-left">
					<h3>新增用户管理信息</h3>
				</div>
				<div class="pull-right">[@p.history_back/]</div>
			</div>
			<!-- 新增表单 -->
			[@p.form id="jvForm" action="sysUser/save.do"]
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>用户名</label>
						<input type="text" class="form-control" name="userid"  value=""/>	
					</div>
					<div class="form-group col-sm-12 col-lg-3">
						<label>密码</label>
						<input type="text" class="form-control" name="pwd"  value=""/>	
					</div>
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>姓名</label>
						<input type="text" class="form-control" name="name"  value=""/>	
					</div>
					<div class="form-group col-sm-12 col-lg-3">
						<label>性别</label>
						[@p.constant_radio code_type="SEX" id="sex" name="sex" value="1"/]
					</div>
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>身份证号</label>
						<input type="text" class="form-control" name="idCard"  value=""/>	
					</div>
					<div class="form-group col-sm-12 col-lg-3">
						<label>地区</label>
						<input type="text" class="form-control" name="area"  value=""/>	
					</div>
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>详细地址</label>
						<input type="text" class="form-control" name="address"  value=""/>	
					</div>
					<div class="form-group col-sm-12 col-lg-3">
						<label>电子邮件</label>
						<input type="text" class="form-control" name="email"  value=""/>	
					</div>
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>手机</label>
						<input type="text" class="form-control" name="phone"  value=""/>	
					</div>
					<!-- <div class="form-group col-sm-12 col-lg-3">
						<label>用户头像</label>
						<input type="text" class="form-control" name="avatar"  value=""/>	
					</div> -->
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>出生日期</label>
						<input type="text" id="datetimepicker" class="form-control"  name="birth"  value=""/>	
					</div>
					<!-- <div class="form-group col-sm-12 col-lg-3">
						<label>createtime</label>
						<input type="text" class="form-control" name="createtime"  value=""/>	
					</div> -->
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>账号状态</label>
						[@p.constant_radio code_type="USER_STATUS" id="userstatus" name="userstatus" value="USING"/]
					</div>
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>账号类型</label>
						[@p.constant_radio code_type="USER_TYPE" id="usertype" name="usertype" value="1"/]
					</div>
				</div>	
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>所在单位</label>
						<input type="text" class="form-control" name="orgid"  value=""/>	
					</div>
					<!-- <div class="form-group col-sm-12 col-lg-3">
						<label>creater</label>
						<input type="text" class="form-control" name="creater"  value=""/>	
					</div> -->
				</div>
				<!-- <div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>createTime</label>
						<input type="text" class="form-control" name="createTime"  value=""/>	
					</div>
					<div class="form-group col-sm-12 col-lg-3">
						<label>modifier</label>
						<input type="text" class="form-control" name="modifier"  value=""/>	
					</div>
				</div> -->
				<div class="clearfix">
					<!-- <div class="form-group col-sm-12 col-lg-3">
						<label>modifyTime</label>
						<input type="text" class="form-control" name="modifyTime"  value=""/>	
					</div> -->
				</div>
				<div class="clearfix">
					<div class="col-sm-12 col-lg-8">[@p.submit/][@p.cancel/]</div>
				</div>
				[/@p.form]
		</div>
	</div>
</body>
</html>
