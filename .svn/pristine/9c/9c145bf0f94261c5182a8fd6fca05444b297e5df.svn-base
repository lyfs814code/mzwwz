<!DOCTYPE html>
<html>
<head>
</head>
<body>
	<div class="row">
		<div class="col-lg-12">
			<!-- 页面信息 -->
			<div class="console-title console-title-border clearfix">
				<div class="pull-left">
					<h3>区县动态详情</h3>
				</div>
				<div class="pull-right">[@p.history_back/]</div>
			</div>
			<!-- 详细表单 -->
			[@p.form id="" action=""]
				<div class="clearfix">
					<div class="form-group col-lg-3">	
						<label><span>*</span>新闻类型:</label>
						<input type="text" class="form-control" value=
						"[@p.constant_value code_type='NEWS_TYPE' name='newsType' value='${news.newsType!}'/]" 
						 disabled="disabled"/>
					</div>							
				</div>
				<div class="clearfix">
					<div class="form-group col-lg-3">	
						<label><span>*</span>标题:</label>	
						<input type="text" class="form-control" value="${news.title!}"  disabled="disabled"/>			
					</div>		
					<br>					
				</div>
				<div class="clearfix">
				<div class="form-group col-lg-3">	
						<label><span>*</span>来源:</label>	
						<input type="text" class="form-control" value="${news.source!}"  disabled="disabled"/>		
				</div>	
				<div class="form-group col-lg-3">	
					<label><span>*</span>是否发布:</label>
					<input type="text" class="form-control" value=
					"[@p.constant_value code_type='IS_PUBLISH' id='publishFlag' name='publishFlag' value='1'/]"  
					disabled="disabled"/>	
				</div>
				</div>	
				<div class="clearfix" style="margin-left: 15px;">
					<label><span>*</span>新闻内容</label>				
					<div class="main_block">
					<pre style="width: 40%">${news.detail!}</pre>
					</div>	
				</div>
				<div class="clearfix">
				<div class="form-group col-lg-3">	
						<label><span>*</span>创建人：</label>	
						<input type="text" class="form-control" value="${news.creater!}"  disabled="disabled"/>	
					</div>							
					<div class="form-group col-lg-3">	
						<label><span>*</span>创建时间：</label>	
						<input type="text" class="form-control" value="${(news.createTime?string('yyyy-MM-dd'))!''}"  disabled="disabled"/>			
					</div>	
				</div>
											
				<div class="clearfix">
					<div class="form-group col-lg-3">	
						<label><span>*</span>修改人:${news.modifier!}</label>	
						<input type="text" class="form-control" value="${news.modifier!}"  disabled="disabled"/>			
					</div>							
					<div class="form-group col-lg-3">	
						<label><span>*</span>修改时间：</label>		
						<input type="text" class="form-control" value="${(news.modifyTime?string('yyyy-MM-dd'))!''}"  disabled="disabled"/>		
					</div>	
				<div class="clearfix">
					<div class="col-sm-12 col-lg-8">[@p.btn_detail ctr="newMzyw"  id="${news.id}" parID="id"/]</div>
				</div>
			[/@p.form]
		</div>
	</div>
</body>
</html>
