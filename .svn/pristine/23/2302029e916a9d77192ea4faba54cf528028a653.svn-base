<!DOCTYPE html>
<html>
<head>
</head>
<body>
	<div class="row">
		<div class="col-lg-12">
			<!-- 页面信息 -->
			<div class="console-title console-title-border clearfix">
				<div class="pull-left">
					<h3>newsBookImg信息</h3>
				</div>
				<div class="pull-right">[@p.history_back/]</div>
			</div>
			<!-- 详细表单 -->
			[@p.form id="" action=""]
				<div class="clearfix">
					<div class="form-group col-lg-3">	
						<label><span>*</span>id</label>			
						<input type="text" class="form-control" value="${newsBookImg.id!}" />						
					</div>							
					<div class="form-group col-lg-3">	
						<label><span>*</span>bookId</label>			
						<input type="text" class="form-control" value="${newsBookImg.bookId!}" />						
					</div>							
				</div>
				<div class="clearfix">
					<div class="form-group col-lg-3">	
						<label><span>*</span>imgUrl</label>			
						<input type="text" class="form-control" value="${newsBookImg.imgUrl!}" />						
					</div>							
				</div>
				<div class="clearfix">
					<div class="col-sm-12 col-lg-8">[@p.btn_detail ctr="NewsBookImg"  id="${newsBookImg.id}" parID="id"/]</div>
				</div>
			[/@p.form]
		</div>
	</div>
</body>
</html>
