<!DOCTYPE html>
<html>
<head>
</head>
<body>
	<div class="row">
		<div class="col-lg-12">
			<!-- 页面信息 -->
			<div class="console-title console-title-border clearfix">
				<div class="pull-left">
					<h3>news信息</h3>
				</div>
				<div class="pull-right">[@p.history_back/]</div>
			</div>
			<!-- 详细表单 -->
			[@p.form id="" action=""]
				<div class="clearfix">
					<div class="form-group col-lg-3">	
						<label><span>*</span>id</label>			
						<input type="text" class="form-control" value="${news.id!}" />						
					</div>							
					<div class="form-group col-lg-3">	
						<label><span>*</span>newsType</label>			
						<input type="text" class="form-control" value="${news.newsType!}" />						
					</div>							
				</div>
				<div class="clearfix">
					<div class="form-group col-lg-3">	
						<label><span>*</span>title</label>			
						<input type="text" class="form-control" value="${news.title!}" />						
					</div>							
					<div class="form-group col-lg-3">	
						<label><span>*</span>source</label>			
						<input type="text" class="form-control" value="${news.source!}" />						
					</div>							
				</div>
				<div class="clearfix">
					<div class="form-group col-lg-3">	
						<label><span>*</span>detail</label>			
						<input type="text" class="form-control" value="${news.detail!}" />						
					</div>							
					<div class="form-group col-lg-3">	
						<label><span>*</span>publishFlag</label>			
						<input type="text" class="form-control" value="${news.publishFlag!}" />						
					</div>							
				</div>
				<div class="clearfix">
					<div class="form-group col-lg-3">	
						<label><span>*</span>creater</label>			
						<input type="text" class="form-control" value="${news.creater!}" />						
					</div>							
					<div class="form-group col-lg-3">	
						<label><span>*</span>createTime</label>			
						<input type="text" class="form-control" value="${(news.createTime?string('yyyy-MM-dd'))!''}" />						
					</div>							
				</div>
				<div class="clearfix">
					<div class="form-group col-lg-3">	
						<label><span>*</span>modifier</label>			
						<input type="text" class="form-control" value="${news.modifier!}" />						
					</div>							
					<div class="form-group col-lg-3">	
						<label><span>*</span>modifyTime</label>			
						<input type="text" class="form-control" value="${(news.modifyTime?string('yyyy-MM-dd'))!''}" />						
					</div>							
				</div>
				<div class="clearfix">
					<div class="col-sm-12 col-lg-8">[@p.btn_detail ctr="News"  id="${news.id}" parID="id"/]</div>
				</div>
			[/@p.form]
		</div>
	</div>
</body>
</html>
