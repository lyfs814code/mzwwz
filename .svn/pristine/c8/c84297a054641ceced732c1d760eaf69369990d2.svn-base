<!DOCTYPE html>
<html>
<head>
<script type="text/javascript">
	$(function() {
		$("#jvForm").bootstrapValidator({
			feedbackIcons: {
	            valid: 'glyphicon glyphicon-ok',
	            invalid: 'glyphicon glyphicon-remove',
	            validating: 'glyphicon glyphicon-refresh'
	        },
			fields: {
								id:{
	            	validators: {
	                	
	            	}
	            },
								name:{
	            	validators: {
	                	
	            	}
	            },
								imgTitle:{
	            	validators: {
	                	notEmpty: {
	                        message: '图片不能为空'
	                    }
	            	}
	            },
								publishFlag:{
	            	validators: {
	                	
	            	}
	            },
								creater:{
	            	validators: {
	                	
	            	}
	            },
								createTime:{
	            	validators: {
	                	
	            	}
	            },
								modifier:{
	            	validators: {
	                	
	            	}
	            },
								modifyTime:{
	            	validators: {
	                	
	            	}
	            },
							},
			submitHandler: function(validator, form, submitButton) {
				$.web.submit("jvForm");
			}
		});
	});
</script>
</head>
<body>
	<div class="row">
		<div class="col-lg-12">
			<!-- 页面信息 -->
			<div class="console-title console-title-border clearfix">
				<div class="pull-left">
					<h3>newsBook信息修改</h3>
				</div>
				<div class="pull-right">[@p.history_back/]</div>
			</div>
			<!-- 编辑表单 -->
			[@p.form id="jvForm" action="newsBook/update.do"]
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3" style="display: none;">
						<label>id</label>				
						<input type="text" class="form-control" name="id"  value="${newsBook.id!}"/>
					</div>							
					<div class="form-group col-sm-12 col-lg-3">
						<label>名称</label>				
						<input type="text" class="form-control" name="name"  value="${newsBook.name!}"/>
					</div>							
				</div>
				<div class="clearfix">
											
					<div class="form-group col-sm-12 col-lg-3">
						<label>发布状态</label>				
						[@p.constant_radio code_type="IS_PUBLISH" id="publishFlag" name="publishFlag" value="${newsBook.publishFlag!}"/]
					</div>							
				</div>
				
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>图标</label>				
						[@p.upload_image name="imgTitle" src="${newsBook.imgTitle!}" item="1" value="${newsBook.imgTitle!}" /]
					</div>	
				</div>
				<div class="clearfix">
					<div class="form-group col-sm-12 col-lg-3">
						<label>修改人</label>				
						<input type="text" class="form-control" name="modifier"  value="${newsBook.modifier!}"/>
					</div>							
				</div>
				<div class="clearfix">
				<div class="col-sm-12 col-lg-8">[@p.submit /][@p.cancel/]</div>
			</div>
			[/@p.form]
		</div>
	</div>
</body>
</html>
</html>
